{
  "version": 1,
  "projects": {
    "lp": {
      "root": "apps/lp",
      "sourceRoot": "apps/lp/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/lp/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "lp:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "lp:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/lp/tsconfig.json", "apps/lp/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/lp/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/lp/jest.config.js",
            "tsConfig": "apps/lp/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "lp-e2e": {
      "root": "apps/lp-e2e",
      "sourceRoot": "apps/lp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/lp-e2e/cypress.json",
            "tsConfig": "apps/lp-e2e/tsconfig.e2e.json",
            "devServerTarget": "lp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/lp-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/lp-e2e/**/*"]
          }
        }
      }
    },
    "client": {
      "root": "apps/client",
      "sourceRoot": "apps/client/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/client",
            "index": "apps/client/src/index.html",
            "main": "apps/client/src/main.tsx",
            "polyfills": "apps/client/src/polyfills.ts",
            "tsConfig": "apps/client/tsconfig.app.json",
            "assets": ["apps/client/src/favicon.ico", "apps/client/src/assets"],
            "styles": ["apps/client/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "client:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/client/tsconfig.app.json",
              "apps/client/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/client/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/client/jest.config.js",
            "tsConfig": "apps/client/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-e2e": {
      "root": "apps/client-e2e",
      "sourceRoot": "apps/client-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/client-e2e/cypress.json",
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json",
            "devServerTarget": "client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/client-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/client-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "schematics": {
    "@nrwl/gatsby": {
      "application": {}
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "lp"
}
